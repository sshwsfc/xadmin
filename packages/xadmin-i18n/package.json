{
  "name": "xadmin-i18n",
  "version": "3.0.0",
  "description": "Xadmin i18n",
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "sshwsfc@gmail.com",
  "license": "MIT",
  "dependencies": {
    "i18next": "^15.0.6",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-localstorage-cache": "^1.1.1",
    "i18next-xhr-backend": "^2.0.1"
  },
  "peerDependencies": {
    "react": "16",
    "lodash": "^4.17.11",
    "moment": "2",
    "xadmin": "2 || 3"
  },
  "scripts": {
    "build:lib": "babel src --root-mode upward --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --root-mode upward --out-dir es",
    "build:umd": "rollup -c ../../rollup.config.js",
    "clean": "rm -rf lib es dist",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run build:umd"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ]
}
