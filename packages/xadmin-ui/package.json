{
  "name": "xadmin-ui",
  "version": "3.3.1",
  "description": "Xadmin Base UI",
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "sshwsfc@gmail.com",
  "license": "MIT",
  "dependencies": {
    "react-redux-loading-bar": "^4.2.0"
  },
  "peerDependencies": {
    "react": "16",
    "lodash": "^4.17.11",
    "react-redux": "^7.0.0",
    "react-router": "6",
    "react-router-dom": "6",
    "xadmin": "2||3",
    "redux-saga": "^1.0.2"
  },
  "scripts": {
    "build:lib": "babel src --root-mode upward --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --root-mode upward --out-dir es",
    "build:umd": "rollup -c ../../rollup.config.js",
    "clean": "rm -rf lib es dist",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run build:umd"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ]
}
