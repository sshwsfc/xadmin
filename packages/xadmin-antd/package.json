{
  "name": "xadmin-antd",
  "version": "3.3.1",
  "description": "Xadmin Antd UI",
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "sshwsfc@gmail.com",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "@ant-design/icons": "^4.0.3",
    "antd": "4",
    "react": ">=16.8",
    "lodash": "4",
    "moment": "2",
    "react-router": "6",
    "react-router-dom": "6",
    "redux-saga": "^1.0.2",
    "xadmin": "2 || 3",
    "xadmin-auth": "3",
    "xadmin-i18n": "3",
    "xadmin-form": "3",
    "xadmin-model": "3",
    "xadmin-ui": "3"
  },
  "scripts": {
    "build:lib": "babel src --root-mode upward --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --root-mode upward --out-dir es",
    "build:umd": "rollup -c ../../rollup.config.js",
    "clean": "rm -rf lib es dist",
    "copyCss": "cp src/layout/layout.css es/layout/ && cp src/layout/layout.css lib/layout/",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run copyCss && npm run build:umd"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ]
}
